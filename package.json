{
  "name": "nyu-cs-wiki",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "npm run packages && docusaurus start",
    "build": "npm run packages && docusaurus build",
    "packages": "node utils/license.js . docs/packages.md",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "rm docs/packages.md && docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "lint-fix": "eslint --fix \"**/*.{js,jsx}\"",
    "format": "prettier --write \"**/*.{js,jsx,css,json}\""
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.1",
    "@docusaurus/plugin-content-docs": "^2.4.1",
    "@docusaurus/preset-classic": "^2.4.1",
    "@docusaurus/theme-mermaid": "^2.4.1",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "license-checker": "^25.0.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/eslint-plugin": "^2.4.1",
    "@docusaurus/module-type-aliases": "2.4.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
